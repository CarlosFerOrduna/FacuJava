package domain;

import java.util.List;

public class Mascota {

	private String nombre;
	private int edad;
	private String raza;
	private Foto fotoPerfil;
	private List<Foto> fotos;

	public Mascota(String nombre, int edad, String raza, Foto fotoPerfil) {
		this.nombre = nombre;
		this.edad = edad;
		this.raza = raza;
		this.fotoPerfil = fotoPerfil;
	}

	public String getNombre() {
		return nombre;
	}

	public int getEdad() {
		return edad;
	}

	public String getRaza() {
		return raza;
	}

	public Foto getFotoPerfil() {
		return fotoPerfil;
	}

	public List<Foto> getFotos() {
		return fotos;
	}

}
